    "def simple_interest(p,t,r): \n",
    "    print('The principal is', p) \n",
    "    print('The time period is', t) \n",
    "    print('The rate of interest is',r) \n",
    "    si = (p * t * r)/100    \n",
    "    print('The Simple Interest is', si) \n",
    "    return si  \n",
    "simple_interest(4,6,8)"
   ]
